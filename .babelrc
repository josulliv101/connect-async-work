{
  "presets": [
    "./tools/es2015-preset",
    "stage-0",
    "react"
  ],
  "env": {
    "development": {
      "plugins": [
        ["transform-runtime", {
          helpers: false,
          polyfill: false,
          regenerator: true }]
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}